#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/dist/bin/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/dist/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/transliteration@2.3.5/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/dist/bin/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/dist/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/transliteration@2.3.5/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../transliteration@2.3.5/node_modules/transliteration/dist/bin/slugify" "$@"
else
  exec node  "$basedir/../../../../../transliteration@2.3.5/node_modules/transliteration/dist/bin/slugify" "$@"
fi
