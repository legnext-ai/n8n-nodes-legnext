#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/@n8n+node-cli@0.17.0_eslint@9.32.0_typescript@5.9.2/node_modules/@n8n/node-cli/bin/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/@n8n+node-cli@0.17.0_eslint@9.32.0_typescript@5.9.2/node_modules/@n8n/node-cli/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/@n8n+node-cli@0.17.0_eslint@9.32.0_typescript@5.9.2/node_modules/@n8n/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/@n8n+node-cli@0.17.0_eslint@9.32.0_typescript@5.9.2/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/@n8n+node-cli@0.17.0_eslint@9.32.0_typescript@5.9.2/node_modules/@n8n/node-cli/bin/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/@n8n+node-cli@0.17.0_eslint@9.32.0_typescript@5.9.2/node_modules/@n8n/node-cli/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/@n8n+node-cli@0.17.0_eslint@9.32.0_typescript@5.9.2/node_modules/@n8n/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/@n8n+node-cli@0.17.0_eslint@9.32.0_typescript@5.9.2/node_modules:/home/tobias/projects/API/n8n-nodes-legnext/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@n8n/node-cli/bin/n8n-node.mjs" "$@"
else
  exec node  "$basedir/../@n8n/node-cli/bin/n8n-node.mjs" "$@"
fi
